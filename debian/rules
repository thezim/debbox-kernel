#!/usr/bin/make -f

build:
	$(MAKE) KERNELRELEASE=4.19.152-ui-alpine ARCH=arm64 	KBUILD_BUILD_VERSION=4.19.152 KBUILD_SRC=

binary-arch:
	$(MAKE) KERNELRELEASE=4.19.152-ui-alpine ARCH=arm64 	KBUILD_BUILD_VERSION=4.19.152 KBUILD_SRC= intdeb-pkg

clean:
	rm -rf debian/*tmp debian/files
	$(MAKE) clean

binary: binary-arch
